{
    "latex-workshop.bibtex-format.case": "UPPERCASE",
    "latex-workshop.intellisense.citation.backend": "biblatex",
    "latex-workshop.intellisense.citation.label": "title",
    "latex-workshop.latex.outDir": "%DIR%/out",
    "latex-workshop.latex.autoBuild.run": "never",
    "latex-workshop.latex.build.rootfileInStatus": true,
    "latex-workshop.latex.rootFile.useSubFile": false,
    "latex-workshop.latex.rootFile.doNotPrompt": true,
    "latex-workshop.latex.option.maxPrintLine.enabled": false,
    "latex-workshop.synctex.afterBuild.enabled": true,
    "latex-workshop.synctex.indicator.enabled": true,
    "latex-workshop.view.outline.sync.viewer": true,
    "latex-workshop.view.outline.commands": [],
    "latex-workshop.view.outline.floats.enabled": false,
    "latex-workshop.view.outline.numbers.enabled": false,
    "latex-workshop.view.autoFocus.enabled": true,
    "latex-workshop.view.outline.sections": [
        "part",
        "chapter",
        "section",
        "subsection"
    ],
    "latex-workshop.latex.recipes": [
        {
            "name": "latexmk",
            "tools": [
                "latexmk"
            ]
        },
    ],
    "latex-workshop.latex.tools": [
        {
            "name": "latexmk",
            "command": "latexmk",
            "args": [
                "%DOC%"
            ],
        },
    ],
    "ltex.additionalRules.enablePickyRules": true,
    "ltex.additionalRules.motherTongue": "de-DE",
    "ltex.additionalRules.languageModel": "~/models/ngrams/",
    "ltex.latex.commands": {
        "\\label{}": "ignore",
        "\\documentclass[]{}": "ignore",
        "\\cite{}": "dummy",
        "\\cite[]{}": "dummy",
        "\\UzLStyle{}": "ignore",
        "\\UzLThesisSetup{}": "ignore",
        "\\subfile{}": "dummy",
        "\\incfig{}": "dummy",
        "\\lstinputlisting[]{}": "ignore",
        "\\newacronym{}{}{}": "ignore",
        "\\gls{}": "dummy",
        "\\glspl{}": "pluralDummy",
        "\\pgfplotsset{}": "ignore",
    },
    "ltex.latex.environments": {
        "lstlisting": "ignore",
        "verbatim": "ignore",
        "Code": "ignore",
    },
    "todo-tree.regex.regex": "((//|#|<!--|;|/\\*|^|%|\\\\)\\s*($TAGS)\\{*|^\\s*- \\[ \\])",
    "todo-tree.general.tags": [
        "BUG",
        "HACK",
        "FIXME",
        "TODO",
        "XXX",
        "[ ]",
        "[x]",
    ],
    "grammarly.selectors": [
        {
            "language": "latex",
            "scheme": "file"
        }
    ],
    "grammarly.config.suggestionCategories.oxfordComma": "on",
    "grammarly.config.documentDialect": "american",
}